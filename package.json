{
	"name": "html-vision",
	"version": "0.3.8",
	"description": "A set of utility functions for html",
	"type": "module",
	"exports": {
		".": {
			"default": "./lib/index.js",
			"types": "./lib/index.d.ts"
		},
		"./click.js": {
			"default": "./lib/click.js",
			"types": "./lib/click.d.ts"
		},
		"./ss.js": {
			"import": "./lib/ss.js",
			"types": "./lib/ss.d.ts"
		}
	},
	"devDependencies": {
		"@vdegenne/mini-gamepad": "^0.4.6",
		"tslib": "^2.8.1",
		"typescript": "^5.6.3",
		"vite": "^6.0.7",
		"wireit": "^0.14.9"
	},
	"scripts": {
		"build": "wireit"
	},
	"wireit": {
		"build": {
			"command": "tsc --pretty",
			"clean": "if-file-deleted",
			"files": [
				"./src",
				"./tsconfig.json"
			],
			"output": [
				"lib"
			]
		}
	},
	"files": [
		"./lib/**/*.js",
		"./lib/**/*.d.ts"
	]
}
